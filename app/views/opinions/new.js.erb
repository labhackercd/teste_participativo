var holder = $("#<%= "buttons-#{params[:related_opinion_id]}" %>");



var form = $("#<%= "form-opinion-#{params[:related_opinion_id]}"%>")

if (form == null)
{
    holder.after("<%= escape_javascript(render 'opinions/new')%>")
}
else
{
    form.remove();
    holder.after("<%= escape_javascript(render 'opinions/new')%>")
}
